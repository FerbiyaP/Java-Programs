

public class Main {
	
	    public static void main(String[] args) {
	        
	        int a = 1234;

	        
	        int num1 = a % 10;          
	        int num2 = (a / 10) % 10;   
	        int num3 = (a / 100) % 10;  
	        int num4 = (a / 1000) % 10; 

	        
	        System.out.println(num1 * 1000 + num2 * 100+ num3 * 10 + num4);

	       
	    }
	

   
}
